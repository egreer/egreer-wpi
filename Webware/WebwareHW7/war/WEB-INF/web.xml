<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">

<!-- Eric Greer -->
	<servlet>
		<servlet-name>AboutMe</servlet-name>
		<servlet-class>hw7.AboutMeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AboutMe</servlet-name>
		<url-pattern>/aboutme</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>MyClasses</servlet-name>
		<servlet-class>hw7.MyClassesServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>MyClasses</servlet-name>
		<url-pattern>/myclasses</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>DidITake</servlet-name>
		<servlet-class>hw7.DidITakeServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>DidITake</servlet-name>
		<url-pattern>/diditake</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
